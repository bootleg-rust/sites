{
  "name": "@bootleg-rust/web-crates-io",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "rushx clean && razzle build && rsync -a ./public/ ./build/public",
    "clean": "rm -rf build",
    "dev": "razzle start",
    "serve": "node build/server.js",
    "service:deploy": "build-tooling-cli service deploy --service-name web-crates-io",
    "service:package": "build-tooling-cli service package --service-name web-crates-io",
    "service:publish": "build-tooling-cli service publish --service-name web-crates-io",
    "test": "jest",
    "verify": "npm-run-all verify:eslint verify:stylelint",
    "verify:eslint": "eslint --max-warnings=0 .",
    "verify:eslint:fix": "eslint --max-warnings=0 --fix .",
    "verify:fix": "npm-run-all verify:eslint:fix",
    "verify:stylelint": "stylelint './src/**/*.{ts,tsx,js,jsx}' "
  },
  "dependencies": {
    "@bootleg-rust/lib-config": "*",
    "@bootleg-rust/lib-design-system": "*",
    "@bootleg-rust/lib-features": "*",
    "@bootleg-rust/lib-ssr-runtime": "*",
    "@bootleg-rust/lib-ssr-toolbox": "*",
    "core-js": "^3.6.4",
    "isomorphic-fetch": "^2.2.1",
    "koa-router": "^7.4.0",
    "md5": "^2.2.1",
    "query-string": "^6.12.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet-async": "^1.0.6",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "regenerator-runtime": "^0.13.7",
    "source-map-support": "^0.5.19",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@bootleg-rust/build-tooling": "*",
    "@jest/globals": "^26.6.2",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@types/koa-router": "^7.4.0",
    "@types/node": "^14.0.26",
    "@types/react": "^16.13.1",
    "@types/react-dom": "^16.9.8",
    "@types/react-router": "^5.1.5",
    "@types/react-router-dom": "^5.1.4",
    "@types/styled-components": "^5.1.1",
    "eslint": "^7.15.0",
    "jest-cli": "26.1.0",
    "npm-run-all": "^4.1.5",
    "razzle": "4.0.0-canary.8",
    "stylelint": "^13.6.1",
    "typescript": "~3.8.3"
  },
  "engines": {
    "node": "14.15",
    "npm": "DO_NOT_USE",
    "pnpm": ">=4.14.4",
    "yarn": "DO_NOT_USE"
  }
}
