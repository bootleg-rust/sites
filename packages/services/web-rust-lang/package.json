{
  "name": "@bootleg-rust/web-rust-lang",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "rushx clean && razzle build --noninteractive && rsync -a ./public/ ./build/public",
    "clean": "rm -rf build",
    "deploy": "build-tooling-cli service deploy --service-name web-rust-lang",
    "dev": "razzle start",
    "serve": "node build/server.js",
    "service:deploy": "build-tooling-cli service deploy --service-name web-rust-lang",
    "service:package": "build-tooling-cli service package --service-name web-rust-lang",
    "service:publish": "build-tooling-cli service publish --service-name web-rust-lang",
    "test": "jest",
    "verify": "npm-run-all verify:eslint verify:stylelint",
    "verify:eslint": "eslint --max-warnings=0 .",
    "verify:eslint:fix": "eslint --max-warnings=0 --fix .",
    "verify:fix": "npm-run-all verify:eslint:fix",
    "verify:stylelint": "stylelint './src/**/*.{ts,tsx,js,jsx}' "
  },
  "dependencies": {
    "@bootleg-rust/base-service-config": "workspace:*",
    "@bootleg-rust/design-system": "workspace:*",
    "@bootleg-rust/features": "workspace:*",
    "@fluent/react": "^0.14.1",
    "@pseudo-su/flex-elements": "workspace:*",
    "@ssr-kit/runtime": "workspace:*",
    "@ssr-kit/toolbox": "workspace:*",
    "core-js": "^3.6.4",
    "framer-motion": "^2.6.1",
    "history": "^5.0.0",
    "isomorphic-fetch": "^2.2.1",
    "koa-router": "^10.0.0",
    "md5": "^2.2.1",
    "query-string": "^7.1.1",
    "razzle": "^4.2.17",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet-async": "^1.3.0",
    "react-router": "^6.0.0-beta.0",
    "react-router-dom": "^6.0.0-beta.0",
    "regenerator-runtime": "^0.13.7",
    "source-map-support": "^0.5.21",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@bootleg-rust/build-tooling": "workspace:*",
    "@jest/globals": "^28.1.3",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^11.2.5",
    "@types/koa-router": "^7.4.0",
    "@types/node": "^14.0.26",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.25",
    "eslint": "^7.15.0",
    "jest-cli": "^28.1.3",
    "npm-run-all": "^4.1.5",
    "stylelint": "^14.9.1",
    "typescript": "~3.8.3"
  },
  "engines": {
    "node": "18.6",
    "npm": "DO_NOT_USE",
    "pnpm": ">=7.6.0",
    "yarn": "DO_NOT_USE"
  }
}
